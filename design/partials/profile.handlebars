<div class="profile stream">
    {{> profileHeader actor=actor}}
    <header>
        <span>🏠 Latest</span>
        <a href="/private" id="newPosts" class="unread" hidden></a>
    </header>
    <div id="profile_stream">
        {{#each activitystream}}
            {{#with this}}
                <div class="activity">
                    {{#if boost}}
                        <div class="boost">
                        🚀 boosted by <a href="{{booster.url}}">{{booster.name}}</a>
                        </div>
                    {{/if}}
                    {{> note note=note}}
                </div>
            {{/with}}
        {{/each}}
    </div>
</div>
